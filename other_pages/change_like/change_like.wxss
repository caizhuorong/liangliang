@import '../../app.wxss';
.btn{
  width: 620rpx;
  height: 82rpx;
  position: fixed;
  left: 65rpx;
  bottom: 85rpx;
  box-shadow: #999 0 8px 15px -10px;
   border-radius: 6rpx;
}
.btn>text{
  font-size: 36rpx;
  color: #fff;
  display: block;
  text-align: center;
  line-height: 82rpx;
}
page{
  box-shadow:0rpx 6rpx 15rpx RGBA(165, 165, 165, 0.1) inset; 
  background: #f8f8f8;
}
.content{
  width: 100%;
  overflow: hidden;
  margin-top: 60rpx;
  background-color: #fff;
}
.content>text{
  color: #959595;
  font-size: 28rpx;
  padding-left: 53rpx;
  display: block;
  padding-top: 37rpx;
}
.layer_label_main{
  display: flex;
  width: 646rpx;
  flex-direction: row;
  justify-content: flex-start;
  flex-wrap:wrap;
  align-items: center;
  margin: 0 auto;
  margin-top: 40rpx;
  margin-bottom: 80rpx;
}
.layer_label_main>view{
  width:130rpx;
  height: 44rpx;
  background-color: #f8f8f8;
  margin-right: 42rpx;
  margin-bottom: 42rpx;
  font-size: 24rpx;
  text-align: center;
  color: #959595;
  line-height: 44rpx;
  border-radius: 6rpx;
}
.layer_label_main>view:nth-child(4n){
  margin-right: 0rpx;
}
.label_active{
  color: #292929 !important;
  font-weight: bold;
  border:1rpx solid #292929;
  width:126rpx !important;
  height: 40rpx !important;
  line-height: 40rpx !important;
}