@import '../../app.wxss';
page{
  box-shadow:0rpx 6rpx 15rpx RGBA(165, 165, 165, 0.1) inset; 
  background: #f8f8f8;
}
.inputs{
  width: 100%;
  height: 102rpx;
  margin-top: 60rpx;
  background-color: #fff;
}
.inputs>input{
  color: #292929;
  font-size: 28rpx;
  padding-left: 30rpx;
  line-height: 102rpx;
  display: block;
  height: 102rpx;
  width: 100%;
}
.btn{
  width: 620rpx;
  height: 82rpx;
  position: fixed;
  left: 65rpx;
  bottom: 85rpx;
  box-shadow: #999 0 8px 15px -10px;
   border-radius: 6rpx;

}
.btn>text{
  font-size: 36rpx;
  color: #fff;
  display: block;
  text-align: center;
  line-height: 82rpx;
}