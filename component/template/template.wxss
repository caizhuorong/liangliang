.icon{
  width:40rpx;
  height: 40rpx;
  margin:0 auto;
}
.tabBar{
  width:100%;
  height: 98rpx;
  position: fixed;
  bottom:0;
  background:#fff;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  border: 1rpx solid #eee;
}
.tabBar-item{
  width:25%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  position: relative;
  height: 100%;
}
.tabBartext{
  color:red;
}
.navig{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0rpx;
  top: 0rpx;
}
.active_itemss{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0rpx;
  top: 0rpx;
}
.iis{
  width:100%;
  height: 100%;
  position: absolute;
  left: 0rpx;
  top: 0rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}