.label_content{
  width: 94%;
  margin: 0 auto;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  
}
.label_title{
  margin-left: 20rpx;
}
.label_title>text{
  font-size: 28rpx;
  color: #292929;
}
.label_value{
  margin-right: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  align-items: center;
}
.label_value>text{
  color: #959595;
  font-size: 28rpx;
  padding-right: 35rpx;
  width: 350rpx;
  display: block;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.ava{
  width: 79rpx;
  height: 79rpx;
  margin-right: 35rpx;
  border-radius: 50%;
}
.more{
  width: 14rpx;
  height: 25rpx;
}