/**app.wxss**/
@import '/component/template/template.wxss';
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.theme_color{
  background-color: #D75294 !important;
}
.theme_colors{
  color: #D75294 !important;
}
.tarb{
  width: 100%;
  height: 98rpx;
}
.navigator-hover{
  background-color: rgba(0,0,0,0) !important;
  opacity: 1 !important;
}
.share_mask{
  width: 100%;
  height: 100%;
  position: fixed;
  background-color: rgba(0,0,0,.6);
  left: 0rpx;
  top: 0rpx;
  z-index:99
}
.share_content{
  width: 526rpx;
  height: 290rpx;
  position:absolute;
  left: 50%;
  top: 50%;
  margin-top: -145rpx;
  margin-left: -263rpx;
  border-radius: 12rpx;
  background-color: #fff;
  overflow: hidden;
}
.share_top{
  width: 100%;
  height: 193rpx;
  border-bottom: 1rpx solid #bfbfbf;
  overflow: hidden;
}
.share_top_title{
  width: 100%;
  text-align: center;
  font-size: 36rpx;
  color: #292929;
  display: block;
  margin-top: 36rpx;
}
.share_top_content{
  width: 100%;
  text-align: center;
  font-size: 30rpx;
  color: #292929;
  display: block;
  margin-top: 32rpx;
}
.share_bottom{
  width: 100%;
  height: 96rpx;
  display: flex;
  flex-direction: row;
  justify-content:center;
  align-items: center;
}
.share_qx{
  flex-grow: 1;
  font-size: 30rpx;
  color: #D75294;
  text-align: center;
  display: block;
  line-height: 96rpx;
  border-right: 1rpx solid #bfbfbf;
  position: relative;
}
.share_qd{
  flex-grow: 1;
  font-size: 30rpx;
  color: #292929;
  text-align: center;
  display: block;
  line-height: 96rpx;
}
