.btn{
  width: 620rpx;
  height: 82rpx;
  box-shadow: #999 0 8px 15px -10px;
  background-color: #D75294;
  border-radius: 8rpx;
}
.btn>text{
  font-size: 36rpx;
  color: #fff;
  display: block;
  text-align: center;
  line-height: 82rpx;
}
page{
  box-shadow:0rpx 6rpx 15rpx RGBA(165, 165, 165, 0.1) inset; 
}
.welcome{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0rpx;
  top: 0rpx;
  overflow: hidden;
}
.welcome>image{
  width: 100%;
  display: block;
  box-shadow:0px 5rpx 40rpx rgba(0,0,0,.3);
}
.getUserInfoclass{
  width: 100%;
  height: 100%;
  opacity: 0;
  position: absolute;
  left: 0rpx;
  top: 0rpx;
  z-index: 99
}
.bottom{
  width: 100%;
  height: 228rpx;
  background-color: #fff;
  position: fixed;
  left: 0rpx;
  bottom: 0rpx;
  z-index: 1;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}