page{
  box-shadow:0rpx 6rpx 15rpx RGBA(165, 165, 165, 0.1) inset; 
}
.line{
  width: 100%;
  height: 10rpx;
  background-color: #f8f8f8;
}
.label_content{
  width: 94%;
  margin: 0 auto;
  min-height: 100rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  
}
.label_title{
  margin-left: 20rpx;
}
.label_title>text{
  font-size: 28rpx;
  color: #292929;
}
.label_value{
  margin-right: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  align-items: center;
}
.label_value>text{
  color: #959595;
  font-size: 28rpx;
  padding-right: 35rpx;
  display: block;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  width:450rpx;
  text-align:right;
}
.btn{
  width: 100%;
  height: 104rpx;
  position: fixed;
  left: 0rpx;
  bottom: 0rpx;
  background:linear-gradient(to left, #05c4f0 , #3888f8);
}
.btn>text{
  font-size: 36rpx;
  color: #fff;
  display: block;
  text-align: center;
  line-height: 104rpx;
}
.ava{
  width: 79rpx;
  height: 79rpx;
  margin-right: 35rpx;
  border-radius: 50%;
}
.more{
  width: 14rpx;
  height: 25rpx;
}