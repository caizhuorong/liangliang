  align-items: center;
  z-index: 88;
  border-radius: 8rpx;
}
.content_btn_row{
  display: flex;
  flex-direction: row;
  justify-content:flex-start;
  align-items: center;
  height: 54rpx;
  width: 100%;
}
.content_btn_row>view{
  width: 50%;
  font-size: 32rpx;
  color: #FFFFFF;
  text-align: center;
  line-height: 54rpx;
}
.content_btn_row>view:nth-child(2){
  border-left: 1rpx solid #fff; 
}
.tips_text{
  width: 100%;
  height: 100rpx;
  background-color: RGBA(248, 248, 248, 1);
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.tips_text>view:nth-child(1){
  width: 455rpx;
  margin-left: 40rpx;
}
.tips_text>view:nth-child(1)>text{
  color: #959595;
  font-size: 24rpx;
  display: block;
}
.tips_text>view:nth-child(2){
  width: 180rpx;
  height: 48rpx;
  border-radius: 6rpx;
  background-color: #ffbd5c;
  margin-right: 48rpx;
}
.tips_text>view:nth-child(2)>text{
  color: #fff;
  font-size: 24rpx;
  display: block;
  text-align: center;
  line-height: 48rpx;
}
.mask{
  position: fixed;
  z-index: 99;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.4);
  top: 0rpx;
  left: 0rpx;
  overflow: hidden;
}
.new{
  position: absolute;
