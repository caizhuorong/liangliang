.contents{
  margin: 20rpx 20rpx;
  text-align: justify;
}  
.item{
  background-color: #fff;
  margin-bottom: 30rpx;
  display: inline-block;
  width:100%;
  border-radius: 8rpx;
  overflow: hidden;
  box-shadow: rgba(153,153,153,.4) 0 10px 30px -10px;
   position: relative;
}
.item-ava{
  width: 40rpx;
  height: 40rpx;
  border-radius: 20rpx;
}
.heart{
  width: 22rpx;
  height: 14rpx;
  margin-right: 8rpx;
}
.heart_{
  display: flex;
  align-items: center;
}
.item-img{
  width: 100%
  
}
.item-imgs{
  width: 38rpx;
  height: 38rpx;
  display: block;
  position: absolute;
  right: 0rpx;
  top: 0rpx;
}
.item-title{
  font-size: 24rpx;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  font-family: 'PingFang SC-Medium';
  color: #1e1e1e;
  margin: 15rpx;
  line-height: 27rpx;
}
.item .name{
  display: flex;
  padding: 0 20rpx;
  margin-top: 20rpx;
  padding-bottom: 22rpx;
  align-items: center;
  font-size: 22rpx;
  color: #1e1e1e;
  font-family: 'PingFang SC-Medium';
}

.item-title-box{
  display: flex;
  position: relative;
}
.name-title{
  flex: 1;
  margin-left: 18rpx;
  margin-right: 20rpx;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  font-size: 24rpx;
  color: #292929;
}
.name text:last-child{
  color: #292929;
  line-height: 10rpx;
  
}
.left,.right{
  display: inline-block;
  vertical-align: top;
  width: 48.5%;
}
.right{
  float: right;
}
.content_btn_position{
  position: fixed;
  bottom: 28rpx;
  left: 65rpx;
  width: 620rpx;
  height: 82rpx;
