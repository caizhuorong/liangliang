.shaow{
  box-shadow:0rpx 6rpx 15rpx RGBA(165, 165, 165, 0.1) inset; 
}
.pic{
  width:100%;
  height: 750rpx;
  position: relative;
  left: 0rpx;
  top: 0rpx;
  box-shadow:0rpx 6rpx 15rpx RGBA(165, 165, 165, 0.1) inset;
  background-size:100% 100%;
}
.pic>image{
  width: 100%;
  height: 100%;
}
.left_icon{
  position: absolute;
  left: 32rpx;
  top: 24rpx;
}
.left_icon>image{
  width: 54rpx;
  height: 54rpx;
  border-radius: 50%;
  margin-left: -10rpx;
}
.right_icon{
  position: absolute;
  right: 20rpx;
  top: 18rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
}
.right_icon>view{
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  margin-bottom: 20rpx;
}
.right_icon>view>image{
  width: 64rpx;
  height: 64rpx;
  margin-bottom: 10rpx;
}
.right_icon>view>text{
  color: #959595;
  font-size: 20rpx;
}
.icon_img_right{
  width: 150rpx;
  height: 150rpx;
  position: absolute;
  background-image:url("data:image/png;base64,");
  background-position: left;
  background-repeat: no-repeat;
  background-size: 2900%;
}
.heartAnimation {
	display: inline-block;
	animation-name: heartBlast;
	animation-duration: .8s;
	animation-iteration-count: 1;
	animation-timing-function: steps(28);
	background-position: right;
}
@keyframes heartBlast {
	0% {
		background-position: left;
	}

	100% {
		background-position: right
	}
}
.pic_bottom{
  width: 100%;
  height: 500rpx;
  position: absolute;
  bottom: 0rpx;
  left: 0rpx;
}
.scroll-view_H{
  height: 84rpx;
  overflow:hidden;
  margin-left: 40rpx;
  white-space:nowrap;

}
.scr_itme{
  width: 80rpx;
  height: 80rpx;
  margin-right: 40rpx;
  line-height: 84rpx;
  display: inline-block;
}
.label_active{
  border-bottom:4rpx solid #05C4F0; 
}
.label_active>text{
  color: #05C4F0 !important;
}
.scr_itme>text{
  font-size: 28rpx;
  color: #292929;
  display: block;
  text-align: center;
}
.bottom_view{
  width: 100%;
  height: 84rpx;
  border-top-left-radius: 8rpx;
  border-top-right-radius: 8rpx;
  margin-top: 150rpx;
  background-color: #fff;
  border-bottom: 2rpx solid #dadada;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.bottom_sex{
  width: 101rpx;
  height: 45rpx;
  border: 2rpx solid #eee;
  border-radius: 24rpx;
  margin-left: 35rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.bottom_sex>image{
  width: 38rpx;
  height: 38rpx;
}
.dirvid{
  width: 2rpx;
  height: 36rpx;
  background-color: #dadada;
  margin-left: 40rpx;
}
.pic_content{
  height: 460rpx;
  background-color: #fff;
  overflow: hidden;
}
.scroll_s{
  width: 100%;
  margin-top: 28rpx;
  margin-left: 32rpx;
  height: 440rpx;
  overflow:scroll;
}
.srcoll_s_item{
  width: 116rpx;
  height: 116rpx;
  float: left;
  border: 2rpx solid #dadada;
  margin-right: 27rpx;
  position: relative;
}
.scroll_big{
  width: 100%;
  height: 116rpx;
  margin-bottom: 34rpx;  
}
.red_dian{
  position: absolute;
  top: 0rpx;
  right: 0rpx;
  width: 14rpx;
  height: 14rpx;
  border-radius: 50%;
  background-color: #E25252;
}

.srcoll_s_item>image{
  position: absolute;
  bottom: 5rpx;
  right: 0rpx;
  width: 30rpx;
  height: 30rpx;
  display: block;
}