  font-size: 28rpx;
}
.bottom_layer>view:nth-child(1){
  border-top-right-radius: 10rpx;
  border-top-left-radius: 10rpx;
  border-bottom:2rpx solid #dadada;
}
.bottom_layer>view:nth-child(2)>text{
 color: red;
}
.bottom_layer>view:nth-child(2){
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}
.bottom_layer>view:nth-child(3){
  border-radius:10rpx; 
