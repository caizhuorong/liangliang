.del{
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  overflow: hidden;
  position: fixed;
  top: -100rpx;
  left: 0rpx;
  background-color: #E33636;
}
.dels{
  animation:bj .3s linear forwards;
}
@keyframes bj{
    0%{
      top: -100rpx;
    }
    100%{
        top: 0rpx;
    }
}
.del>image{
  width: 50rpx;
  height: 50rpx;
  margin-bottom: 0rpx;
}
.del>text{
  color: #fff;
  text-align: center;
  font-size: 24rpx;
}
.mask{
  position: fixed;
  z-index: 11;
  left: 0rpx;
  top:0rpx;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,.4);
}
.btn_content{
  width: 90%;
  height: 300rpx;
  position: fixed;
  z-index: 12;
  left: 5%;
}
.btn_content>view{
  font-size: 32rpx;
  color: #292929;
  text-align: center;
  height: 90rpx;
  width: 100%;
  background-color: #fff;
  line-height: 90rpx;
  border-bottom:1rpx solid #eee; 
}
.btn_content>view:nth-child(1){
  border-top-right-radius:8rpx;
  border-top-left-radius: 8rpx; 
}
.btn_content>view:nth-child(2){
  border-bottom-right-radius:8rpx;
  border-bottom-left-radius: 8rpx; 
}
.btn_content>view:nth-child(3){
  border-radius: 8rpx;
}
.hiden_content{
 animation: scrollsss .3s linear forwards;
}
@keyframes scrollsss {
  0% {
    bottom:0rpx;
  }
  100% {
    bottom:-390rpx;
  }
}
.show_content{
animation: scrollss .3s linear forwards;
}
@keyframes scrollss {
  0% {
    bottom:-390rpx;
  }
  100% {
    bottom:0rpx;
  }
}
