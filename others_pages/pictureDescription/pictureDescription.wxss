/* pages/ pictureDescription/pictureDescription.wxss */
page{
  padding-bottom: 200rpx;
  box-shadow:0rpx 6rpx 15rpx RGBA(165, 165, 165, 0.1) inset; 
}
.row-flex {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
}
.row {
  display: flex;
  justify-content: center;
  align-items: center;
}
.row {
  justify-content: unset;
  margin: 30rpx 30rpx 0;
  
}
.row-flex{
  flex-direction:column;
}
.title {
  font-size: 36rpx;
  color: rgba(5, 196, 240, 1);
  margin: 0rpx auto;
  margin-top: 56rpx;
  margin-bottom: 47rpx;
  text-align: center;
  overflow:hidden;
}

.title-img {
  width: 690rpx;
  height: 668rpx;
  margin: 0 30rpx;
}

.img-right {
  width: 49rpx;
  height: 47rpx;
}

.text {
  height: 60rpx;
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(34, 34, 34, 1);
  line-height: 60rpx;
  margin-left: 13rpx;
}

.line {
  width: 690rpx;
  height: 1rpx;
  background: rgba(229, 229, 229, 1);
  margin: 100rpx auto 0;
}

.error-img {
  width: 260rpx;
  height: 260rpx;
}
.error-text{
  font-size:26rpx;
color:rgba(147,147,147,1);
margin-top: 29rpx;
margin-bottom:20rpx;
height:60rpx;

}