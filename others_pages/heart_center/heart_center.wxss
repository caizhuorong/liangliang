
@import '../../app.wxss';
.bj{
  width: 100%;
  overflow: hidden;
  background-color: #f8f8f8;
}
.card_bj{
  position: relative;
  height: 431rpx;
  width: 100%;
  background-color: #fff;
  
}
.share-btn {
  width: 100%;
  height: 100%;
  border: none;
  background: #fff;
  outline: none;
  position: absolute;
  right: 0;
  bottom: 0;
  z-index: 2;
  opacity: 0;
}
.btn{
  width: 100%;
  height: 102rpx;
  position: fixed;
  left: 0rpx;
  bottom: 0rpx;
  box-shadow: #999 0 8px 15px -10px;
  background:linear-gradient(to left, #05c4f0 , #3888f8);
}
.btn>text{
  font-size: 36rpx;
  color: #fff;
  display: block;
  text-align: center;
  line-height: 102rpx;
}
.card{
  width:100%;
  height: 431rpx;
  background-color: #fff;
  overflow: hidden;
}
.oladfollow{
  color: #C6C6C6 !important;
}
.card_top{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  height: 270rpx;
  box-shadow:0rpx 6rpx 15rpx RGBA(165, 165, 165, 0.1) inset;
}
.logo{
  width: 190rpx;
  height: 190rpx;
  margin-left: 40rpx;
  border-radius: 50%;
  overflow: hidden;
}
.logo>image{
  width: 190rpx;
  height: 190rpx;
}
.info{
  width: 430rpx;
  margin-left: 19rpx;
  display: flex;
  flex-direction: column;
  justify-content:center;
  align-items: flex-start;
}
.info_one{
}
.info_one>image{
  width: 30rpx;
  height: 30rpx;
  margin-left: 20rpx;
}
.info_one>text{
  font-size: 42rpx;
  color: #292929;
  margin-left: 10rpx;
}
.info_two{
  font-size: 28rpx;
  color: #292929;
  margin-top: 25rpx
}
.info_three{
  font-size: 30rpx;
  color: #292929;
  margin-top: 25rpx;
  width: 95%;
  display:-webkit-box;
word-break:break-all;
-webkit-box-orient:vertical;
-webkit-line-clamp:2;
overflow:hidden;
text-overflow:ellipsis;

}
.card_bottom{
  width: 100%;
  height: 138rpx;
  display: flex;
  flex-direction: row;
  justify-content:flex-start;
  align-items: center;
}
.card_bottom>view{
  width: 33.3%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.card_bottom>view:nth-child(3){
  border-right: none;
}
.card_bottom>view>text{
  font-size: 36rpx;
}
.card_bottom>view>text:nth-child(2){
  font-size: 24rpx;
  padding-top: 10rpx;
}
.my_work{
  width: 100%;
  overflow: hidden;
  height: auto;
  background-color: #fff;
}
.my_work_top{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  height: 104rpx;
  width: 100%;
}
.my_work_top>view{
  width:6rpx;
  height: 22rpx;
  background-color: #05C4F0;
  margin-left: 40rpx;
}
.my_work_top>text{
  margin-left: 8rpx;
  font-size: 32rpx;
  color: #292929;
}
.work_content{
  width:89%;
  margin:0 auto;
  display: flex;
  flex-direction: row;
  justify-content:flex-start;
  align-items: center;
  flex-wrap: wrap;
}
.work_content_item{
  width: 206rpx;
  height: 206rpx;
  position: relative;
  border-radius: 8rpx;
  margin-top: 10rpx;
  display: inline-block;
  margin-right: 25rpx;
  box-shadow: 0 0 8rpx RGBA(36, 56, 103, .3);
  background-size:cover; 
  margin-bottom: 20rpx;
}
.work_content_item:nth-child(3n){
  margin-right:0rpx;
}
.work_content_item>image{
  width: 38rpx;
  height: 38rpx;
  position: absolute;
  top: 0rpx;
  right: 0rpx;
}
.good{
  width: 100%;
  overflow: hidden;
  background-color: #fff;
  margin-top: 10rpx;
}
.good_content{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  margin-left: 65rpx;
  flex-wrap:wrap;
}
.good_list{
  margin-right: 60rpx;
  width: 152rpx;
  height: 56rpx;
  border-radius: 6rpx;
  background-color: #EEEEEE;
  margin-bottom: 36rpx;
}
.good_list>text{
  color: #292929;
  font-size: 28rpx;
  display: block;
  text-align: center;
  line-height: 56rpx;
}
.good_lists{
  margin-right: 15rpx;
  width: 112rpx;
  height: 45rpx;
  border-radius: 6rpx;
  background-color: #FFEDD3;
  margin-bottom: 30rpx;
}
.good_lists>text{
  color: #D37E00;
  font-size: 24rpx;
  display: block;
  text-align: center;
  line-height: 45rpx;
}
.service{
  margin-bottom: 30rpx;
  margin-left: 92rpx;
}
.service>text{
  color: #292929;
  font-size: 28rpx;
  display: block;
  margin-bottom: 15rpx
}
.shop{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  margin-bottom: 20rpx
}
.shop>text{
  color: #292929;
  font-size: 28rpx;
  display: block;
  width: 480rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.shop>view{
  width: 150rpx;
  height: 38rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  background-color: #00C3F0;
  border-radius: 6rpx;
}
.shop>view>image{
  width: 20rpx;
  height: 28rpx;
  margin-right: 10rpx;
}
.shop>view>text{
  color: #fff;
  font-size: 24rpx;
}
.content_btn_position{
  position: fixed;
  bottom: 38rpx;
  left: 65rpx;
  width:620rpx;
  height:82rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 8rpx;
  box-shadow: 0 0 8rpx RGBA(36, 56, 103, .5);
}
.content_btn_row{
  display: flex;
  flex-direction: row;
  justify-content:center;
  align-items: center;
  height: 54rpx;
  width: 100%;
}
.content_btn_row>view{
  width: 50%;
  font-size: 32rpx;
  color: #FFFFFF;
  text-align: center;
  line-height: 54rpx;
}
.content_btn_row>view:nth-child(2){
  border-left: 1rpx solid #fff; 
}
.layer_bj{
  width:100%;
  height: 100%;
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  background-color:  rgba(0,0,0,.4);
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  z-index: 3
}
.layer{
  width: 510rpx;
  height: 720rpx;
  background-color: #fff;
  border-radius: 8rpx;
}
.layer_close{
  float: right;
  width: 34rpx;
  height: 42rpx;
  margin-right: 20rpx;
  margin-top: 20rpx;
  overflow: hidden;

}
.layer_close>image{
  width: 34rpx;
  height: 34rpx;
}
.layer_title{
  display: block;
  font-size: 32rpx;
  color: #292929;
  clear: both;
  text-align: center;
}
.ewm{
  width: 326rpx;
  height: 326rpx;
  display: block;
  margin: 0 auto;
  margin-top: 20rpx;
}
.ewm_tips{
  display: block;
  margin-top: 11rpx;
  text-align: center;
  font-size: 24rpx;
  color:#292929;
}
.ewm_line{
  width: 433rpx;
  height: 2rpx;
  margin: 0 auto;
  margin-top: 35rpx;
  background-color: #dadada;
}
.tel{
  display: flex;
  margin-top: 35rpx;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.tel>image{
  width: 38rpx;
  height: 38rpx;
  margin-right: 14rpx;
}
.tel>text{
  font-size: 32rpx;
  color: #292929;
}
.call_tel{
  width: 260rpx;
  height: 56rpx;
  display: flex;
  margin:0 auto;
  margin-top: 38rpx;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  background:linear-gradient(to left, #05c4f0 , #3888f8);
  border-radius: 6rpx;
}
.call_tel{
  color: #fff;
  font-size: 28rpx; 
}